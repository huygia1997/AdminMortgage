{
	"_version": "1.1.0",
	"sap.app": {
		"id": "sap.ui.demo.toolpageapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"_version": "1.3.0"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal", "sap_belize"],
		"_version": "1.3.0"
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.toolpageapp.view.InitView",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt": {},
				"sap.ui.layout": {},
				"sap.uxap": {},
				"sap.suite.ui.microchart": {
					"lazy": true
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.toolpageapp.i18n.i18n"
				}
			},
			"side": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideContent.json"
			},
			"alerts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/alerts.json"
			},
			"customer": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/customers.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewPath": "sap.ui.demo.toolpageapp.view",
				"clearTarget": false
			},
			"routes": [{
				"pattern": "",
				"name": "login",
				"target": "login"
			}, {
				"pattern": "home",
				"name": "home",
				"target": "home"
			}, {
				"pattern": "shopManager",
				"name": "shopManager",
				"target": "shopManager"
			}, {
				"pattern": "userManager",
				"name": "userManager",
				"target": "userManager"
			}, {
				"pattern": "categoryManager",
				"name": "categoryManager",
				"target": "categoryManager"
			}],
			"targets": {
				"login": {
					"viewName": "LoginView",
					"viewLevel": 1,
					"controlId": "rootApp",
					"controlAggregation": "pages"
				},
				"app": {
					"viewName": "App",
					"viewLevel": 1,
					"controlId": "rootApp",
					"controlAggregation": "pages"
				},
				"home": {
					"parent": "app",
					"viewName": "Home",
					"viewLevel": 2,
					"controlId": "mainContents",
					"controlAggregation": "pages"
				},
				"shopManager": {
					"parent": "app",
					"viewName": "ShopManager",
					"viewLevel": 2,
					"controlId": "mainContents",
					"controlAggregation": "pages"
				},
				"userManager": {
					"parent": "app",
					"viewName": "UserManager",
					"viewLevel": 2,
					"controlId": "mainContents",
					"controlAggregation": "pages"
				},
				"categoryManager": {
					"parent": "app",
					"viewName": "CategoryManager",
					"viewLevel": 2,
					"controlId": "mainContents",
					"controlAggregation": "pages"
				}
			}
		},
		"_version": "1.2.0",
		"contentDensities": {
			"compact": false,
			"cozy": false
		}
	}
}